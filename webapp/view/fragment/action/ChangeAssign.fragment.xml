<c:FragmentDefinition
	xmlns:c="sap.ui.core"
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:zcontrolPosFin="zsap.com.r3.cobi.s4.custposfin.z_s4_zposfinlink.controls"
	xmlns:zcontrollinkSar="custlinksar.custlinksar.controls"
>
	<VBox class="sapUiSmallMargin ">
		<items>
			<l:Grid
				defaultSpan="XL12 L12 M12 S12"
				vSpacing="0"
			>
				<Table
					id="tableChangeAssign"
					width="auto"
					class="sapUiResponsiveMargin"
					items="{
						path: 'ProvisionSave>/'
					}"
					noDataText="{i18n>tableNoDataText}"
					busyIndicatorDelay="{provisionView>/delay}"
					growing="true"
					mode="SingleSelectLeft"
				>
					<headerToolbar>
						<Toolbar>
							<Label
								class="textBig"
								design="Bold"
								text="{provisionView>/titleTable}"
							/>
							<ToolbarSpacer/>
						</Toolbar>
					</headerToolbar>
					<columns>
						<Column
							minScreenWidth="Tablet"
							demandPopin="true"
						>
							<header>
								<Text text="{i18n>tableNameColumnDetailsFipex}"/>
							</header>
						</Column>
						<Column
							minScreenWidth="Tablet"
							demandPopin="true"
						>
							<header>
								<Text text="{i18n>tableNameColumnDetailsFistl}"/>
							</header>
						</Column>
						<Column
							minScreenWidth="Tablet"
							demandPopin="true"
						>
							<header>
								<Text text="{i18n>tableNameColumnDetailsGeber}"/>
							</header>
						</Column>
					</columns>
					<items>
						<ColumnListItem>
							<cells>
								<!-- <Text  text="{ProvisionSave>FipexEng}" /> -->
								<!-- <Text  text="{ProvisionSave>FistlEng}" /> -->
								<zcontrolPosFin:LinkPosizioneFinanziaria
									id="linkPosFinSpesaENGCA"
									anno="{ProvisionSave>FdatkYear}"
									eos="S"
									fikrs="S001"
									fase="GEST"
									reale="R"
									versione="D"
									fipex="{ProvisionSave>FipexEng}"
									datbis="99991231"
									text="{ProvisionSave>FipexEng}"
									semanticObject="ESAMINAMODIFICA"
									wrapping="true"
								/>
								<zcontrollinkSar:LinkSar
									id="linkSarBilancioENGCA"
									anno="{ProvisionSave>FdatkYear}"
									eos="E"
									fikrs="S001"
									fase="GEST"
									SEM_OBJ=""
									reale="R"
									versione="D"
									bilancio=""
									fictr="{ProvisionSave>FistlEng}"
									datbis="99991231"
									text="{ProvisionSave>FistlEng}"
									wrapping="true"
								/>
								<Text text="{ProvisionSave>ZgeberEng}"/>
							</cells>
						</ColumnListItem>
					</items>
				</Table>
			</l:Grid>
		</items>
	</VBox>
</c:FragmentDefinition>